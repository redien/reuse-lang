
(def transform-definitions (definitions)
     ((pipe (list-map (pipe transform-pipe transform-list))
            (list-map definition-to-sexp)
            stringify)
        definitions))

(export to_reuse (definitions _)
        (match (result-concat definitions)
               (Result definitions)  (Result (transform-definitions definitions))
               (Error error)         (Error  (parse-error error))))
