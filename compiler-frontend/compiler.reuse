
(def parse (file-entries)
     (parse-source-files (with-local-transform-keywords (internal-symbols)) file-entries))

(def transform (definitions)
     (match (perform-transformations?)
            False  definitions
            True   (local-transforms (validate-reserved-identifiers definitions))))

(def generate (module-name definitions)
     (result-map (generate-source module-name) definitions))
