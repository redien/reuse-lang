
(def frontend (file-entries)
     ((pipe (parse-source-files (with-local-transform-keywords (internal-symbols)))
            (match (perform-transformations?)
                   False   id
                   True  (. local-transforms validate-reserved-identifiers)))
          file-entries))

(def compile (file-entries module-name)
     (result-map (generate-source module-name)
                 (frontend file-entries)))
