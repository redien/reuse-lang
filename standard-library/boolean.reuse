(pub typ boolean True False)

(pub def not (a)
     (match a
            True False
            False True))

(pub def and (a b)
     (match a
            True b
            False False))

(pub def or (a b)
     (match a
            True True
            False b))

(pub def < (a b)
     (int32-less-than a b True False))

(pub def > (a b)
     (< b a))

(pub def = (a b)
     (not (or (< a b) (> a b))))

(pub def <= (a b)
     (or (< a b) (= a b)))

(pub def >= (a b)
     (or (> a b) (= a b)))

