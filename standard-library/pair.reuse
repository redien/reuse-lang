
(typ (pair a b) (Pair a b))

(def pair-left (pair)
     (match pair
            (Pair x _)  x))

(def pair-right (pair)
     (match pair
            (Pair _ x)  x))

(def pair-map-left (f pair)
     (match pair
            (Pair x y)  (Pair (f x) y)))

(def pair-map-right (f pair)
     (match pair
            (Pair x y)  (Pair x (f y))))
