{
  "name": "reuse-lang",
  "version": "0.1.0",
  "description": "A simple lisp",
  "main": "reuse.js",
  "dependencies": {
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "cucumber": "^1.0.0",
    "mocha": "^2.5.3",
    "promise": "^7.1.1",
    "should": "^9.0.2",
    "temp": "^0.8.3"
  },
  "scripts": {
    "loc": "find . -name '*.js' | grep -v \"./node_modules\" | xargs wc -l",
    "unit-test": "mocha \"**/*.unit.js\"",
    "acceptance-test": "echo && echo Language: Javascript && echo && cucumberjs -r features/step_definitions/javascript.js && echo && echo Language: Rust && echo && cucumberjs -r features/step_definitions/rust.js && echo && echo Language: C && echo && cucumberjs -r features/step_definitions/c.js",
    "test": "npm run-script unit-test && npm run-script acceptance-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redien/reuse-lang.git"
  },
  "keywords": [
    "reuse",
    "language",
    "reuse-lang",
    "lisp",
    "scheme",
    "lang",
    "transpiler",
    "compiler"
  ],
  "author": "Jesper Oskarsson <jesosk@gmail.com>",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/redien/reuse-lang/issues"
  },
  "homepage": "https://github.com/redien/reuse-lang#readme"
}
